{
  "expo": {
    "name": "FE-LifeHub - Your Life, Your Hub",
    "slug": "FE-LifeHub",
    "version": "1.0.0",
    "description": "Your comprehensive life management hub - organize, track, and enhance your daily life with ease",
    "orientation": "portrait",
    "icon": "./assets/images/logoSplash.jpeg",
    "scheme": "felifehub",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logoSplash.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/logoSplash.jpeg",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "tabletImage": "./assets/images/logoSplash.jpeg"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoSplash.jpeg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "splash": {
        "image": "./assets/images/logoSplash.jpeg",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "mdpi": "./assets/images/logoSplash.jpeg",
        "hdpi": "./assets/images/logoSplash.jpeg",
        "xhdpi": "./assets/images/logoSplash.jpeg",
        "xxhdpi": "./assets/images/logoSplash.jpeg",
        "xxxhdpi": "./assets/images/logoSplash.jpeg"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logoSplash.jpeg",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#f0f8ff"        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
